{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "eol-last":"off",
        "linebreak-style":"off",
        "max-len":"off",
        "no-console": "off",
        "parser": "babel-eslint"
    }
}
